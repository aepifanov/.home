

Host *
    ForwardAgent yes
    TCPKeepAlive no
    ServerAliveInterval 300
    ServerAliveCountMax 2
    Compression yes
    Ciphers blowfish-cbc
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    IdentityFile ~/.ssh/id_rsa

Host github.com
    HostName github.com
    IdentityFile ~/.ssh/github_id_dsa

Host bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/github_id_dsa

