

Host *
    ForwardAgent yes
    TCPKeepAlive no
    ServerAliveInterval 300
    ServerAliveCountMax 2
    Compression yes
    Ciphers blowfish-cbc
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    IdentityFile ~/.ssh/id_ssa

Host github.com
    HostName github.com
    IdentityFile ~/.ssh/github_id_dsa

Host srv
    HostName 172.18.184.44

Host srv_root
    User root
    IdentityFile ~/.ssh/andrey.pem
    HostName 172.18.184.44
