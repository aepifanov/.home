

Host *
    IdentityFile ~/.ssh/id_rsa
    TCPKeepAlive no
    ServerAliveInterval 300
    ServerAliveCountMax 2
    Compression yes
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    ForwardAgent yes

Host bitbucket.org
    HostName bitbucket.org
    IdentityFile ~/.ssh/github_id_dsa

